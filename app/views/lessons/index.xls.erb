<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">School</Data></Cell>
        <Cell><Data ss:Type="String">Teacher First Name</Data></Cell>
        <Cell><Data ss:Type="String">Teacher Last Name</Data></Cell>
        <Cell><Data ss:Type="String">Student First Name</Data></Cell>
        <Cell><Data ss:Type="String">Student Last Name</Data></Cell>
        <Cell><Data ss:Type="String">Time In</Data></Cell>
        <Cell><Data ss:Type="String">Time Out</Data></Cell>
        <Cell><Data ss:Type="String">Brought Instrument</Data></Cell>
        <Cell><Data ss:Type="String">Brought Books</Data></Cell>
      <% Goal::MAX_PER_STUDENT.times do |i| %>
        <Cell><Data ss:Type="String">Goal <%= i+1 %> Name</Data></Cell>
        <Cell><Data ss:Type="String">Goal <%= i+1 %> Rating</Data></Cell>
      <% end %>
        <Cell><Data ss:Type="String">Progress</Data></Cell>
        <Cell><Data ss:Type="String">Behavior</Data></Cell>
        <Cell><Data ss:Type="String">Notes</Data></Cell>
        <Cell><Data ss:Type="String">Group Lesson</Data></Cell>
      </Row>
    <% @messons.each do |lesson| %>
      <Row>
        <Cell><Data ss:Type="String"><%= lesson.school.name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.teacher.first_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.teacher.last_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.student.first_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.student.last_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.time_in %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.time_out %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.brought_instrument %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.brought_books %></Data></Cell>
      <% Goal::MAX_PER_STUDENT.times do |i|
          rating = lesson.ratings[i]
          goal = rating ? rating.goal : nil
          goal_name = goal ? goal.name : ""
          score = rating ? rating.score : ""
      %>
        <Cell><Data ss:Type="String"><%= goal_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= score %></Data></Cell>
      <% end %>
        <Cell><Data ss:Type="String"><%= lesson.progress %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.behavior %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.notes %></Data></Cell>
        <Cell><Data ss:Type="String"><%= lesson.group_lesson_id? %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
